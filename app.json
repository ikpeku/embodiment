{
  "expo": {
    "name": "embodiment healthcare",
    "slug": "embodiment",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/appstore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
       "userInterfaceStyle": "light",
      "supportsTablet": true,
      "bundleIdentifier": "com.ppi10.embodiment"
      
    },
    "android": {
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.ppi10.embodiment",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/playstore.png" 
    },
    "extra": {
      "eas": {
        "projectId": "0419553c-e08e-4844-aa92-8aab96522036"
      }
    },
    "plugins": [
      "./plugins/withDisableForcedDarkModeAndroid.js",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
